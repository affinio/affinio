{
  "name": "@affino/datagrid-laravel",
  "version": "0.2.1",
  "description": "Laravel / Livewire adapter facade for Affino DataGrid",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup resources/js/index.ts --format esm,cjs --dts --clean"
  },
  "dependencies": {
    "@affino/datagrid-core": "workspace:*",
    "@affino/datagrid-orchestration": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.5.1"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/affinio/affinio.git",
    "directory": "packages/datagrid-laravel"
  },
  "homepage": "https://github.com/affinio/affinio/tree/main/packages/datagrid-laravel#readme",
  "bugs": {
    "url": "https://github.com/affinio/affinio/issues"
  },
  "keywords": [
    "affino",
    "laravel",
    "livewire",
    "datagrid",
    "adapter",
    "headless"
  ]
}
