{
  "name": "@affino/vue-adapter",
  "version": "0.1.0",
  "description": "Unified Vue adapter bootstrap for Affino UI primitives",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "test": "vitest run"
  },
  "dependencies": {
    "@affino/overlay-host": "workspace:*",
    "@affino/overlay-kernel": "workspace:*"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "vitest": "^4.0.18",
    "vue": "^3.4.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/affinio/affinio.git",
    "directory": "packages/vue-adapter"
  },
  "homepage": "https://github.com/affinio/affinio/tree/main/packages/vue-adapter#readme",
  "bugs": {
    "url": "https://github.com/affinio/affinio/issues"
  },
  "keywords": [
    "affino",
    "vue",
    "vue3",
    "adapter",
    "headless",
    "accessibility"
  ]
}
