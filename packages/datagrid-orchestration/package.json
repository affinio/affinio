{
  "name": "@affino/datagrid-orchestration",
  "version": "0.2.1",
  "author": "Anton Pavlov <a.pavlov@affino.dev>",
  "type": "module",
  "description": "Framework-agnostic orchestration primitives for Affino DataGrid adapters",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./*": {
      "types": "./dist/src/*.d.ts",
      "import": "./dist/src/*.js"
    }
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.public.json",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "type-check:public": "tsc -p tsconfig.public.json --noEmit",
    "test:unit": "vitest run --config vitest.config.ts --passWithNoTests",
    "test:contracts": "vitest run --config vitest.config.ts --testNamePattern=\"contract\" --passWithNoTests",
    "test": "npm run type-check"
  },
  "dependencies": {
    "@affino/datagrid-core": "workspace:*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/affinio/affinio/tree/main/packages/datagrid-orchestration"
  },
  "homepage": "https://affino.dev",
  "license": "MIT",
  "keywords": [
    "datagrid",
    "orchestration",
    "headless",
    "framework-agnostic"
  ],
  "bugs": {
    "url": "https://github.com/affinio/affinio/issues"
  },
  "module": "dist/src/index.js"
}
