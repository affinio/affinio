{
  "name": "@affino/visual-workbench",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "storybook build",
    "serve:storybook": "http-server storybook-static --port 6006 --silent",
    "test:visual:runner": "wait-on tcp:6006 && test-storybook --testTimeout=60000 --maxWorkers=1",
    "test:visual": "storybook build --quiet && concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"pnpm run serve:storybook\" \"pnpm run test:visual:runner\""
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^10.2.10",
    "@storybook/addon-docs": "^10.2.10",
    "@storybook/test-runner": "^0.24.2",
    "@storybook/vue3": "^10.2.10",
    "@storybook/vue3-vite": "^10.2.10",
    "@vitejs/plugin-vue": "^6.0.1",
    "concurrently": "^9.2.1",
    "http-server": "^14.1.1",
    "storybook": "^10.2.10",
    "ts-node": "^10.9.2",
    "vue": "^3.5.25",
    "wait-on": "^7.2.0"
  },
  "description": "@affino/visual-workbench workspace package",
  "version": "0.0.0",
  "license": "MIT"
}
