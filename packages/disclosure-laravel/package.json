{
  "name": "@affino/disclosure-laravel",
  "version": "0.0.1",
  "description": "Livewire disclosure helper wired to @affino/disclosure-core",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup resources/js/index.ts --format esm,cjs --dts --clean",
    "test": "vitest run"
  },
  "dependencies": {
    "@affino/disclosure-core": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "vitest": "^4.0.15"
  }
}
